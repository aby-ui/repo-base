						<Scripts>
							<OnLoad>
								self:SetParent(nil);
							</OnLoad>
						</Scripts>
					</Frame>
				</Frames>
				<Scripts>
					<OnLoad>
						Wholly:ScrollFrameTwo_OnLoad(self)
					</OnLoad>
				</Scripts>
			</ScrollFrame>
		</Frames>
		<Scripts>
			<OnLoad>
				tinsert(UISpecialFrames, self:GetName())
			</OnLoad>
			<OnShow>
				Wholly:OnShow(self)
				PlaySound(PlaySoundKitID and "igCharacterInfoOpen" or 839);
			</OnShow>
			<OnHide>
				Wholly:OnHide(self)
				PlaySound(PlaySoundKitID and "igCharacterInfoClose" or 840);
				if self.isMoving then
					self:StopMovingOrSizing()
					self.isMoving = false
				end
			</OnHide>
			<OnMouseUp>
				if self.isMoving then
					self:StopMovingOrSizing()
					self.isMoving = false
				end
			</OnMouseUp>
			<OnMouseDown>
				if (not self.isLocked or self.isLocked == 0) and button == "LeftButton" then
					self:StartMoving()
					self.isMoving = true
				end
			</OnMouseDown>
		</Scripts>
