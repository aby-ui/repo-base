<Bindings>
	<Binding name="SRTI_SHOW" header="SRTI_TITLE" category="ADDON_LIST">
		SRTI.ShowFromBinding();
	</Binding>
	<Binding name="SRTI_STAR" category="ADDON_LIST">
		SetRaidTarget("mouseover",1);
	</Binding>
	<Binding name="SRTI_CIRCLE" category="ADDON_LIST">
		SetRaidTarget("mouseover",2);
	</Binding>
	<Binding name="SRTI_DIAMOND" category="ADDON_LIST">
		SetRaidTarget("mouseover",3);
	</Binding>
	<Binding name="SRTI_TRIANGLE" category="ADDON_LIST">
		SetRaidTarget("mouseover",4);
	</Binding>
	<Binding name="SRTI_MOON" category="ADDON_LIST">
		SetRaidTarget("mouseover",5);
	</Binding>
	<Binding name="SRTI_SQUARE" category="ADDON_LIST">
		SetRaidTarget("mouseover",6);
	</Binding>
	<Binding name="SRTI_CROSS" category="ADDON_LIST">
		SetRaidTarget("mouseover",7);
	</Binding>
	<Binding name="SRTI_SKULL" category="ADDON_LIST">
		SetRaidTarget("mouseover",8);
	</Binding>
	<Binding name="SRTI_CLEAR" category="ADDON_LIST">
		SetRaidTarget("mouseover",0);
	</Binding>
</Bindings>
