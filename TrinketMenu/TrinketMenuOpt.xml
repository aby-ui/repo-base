	<Button name = "TrinketMenu_IconFrame" frameStrata = "LOW" parent = "Minimap" enableMouse = "true" movable = "true" toplevel = "true" hidden = "false">
		<Size>
			<AbsDimension x = "32" y = "32"/>
		</Size>
		<Anchors>
			<Anchor point = "TOPLEFT"/>
		</Anchors>
		<Scripts>
			<OnLoad>
				self:RegisterForClicks("LeftButtonUp", "RightButtonUp")
				self:RegisterForDrag("LeftButton")
			</OnLoad>
			<OnEnter>
				TrinketMenu.OnTooltip(self,"TrinketMenu",(TrinketMenuOptions.DisableToggle == "ON") and TrinketMenu.Tooltip1 or TrinketMenu.Tooltip2)
			</OnEnter>
			<OnLeave>
				GameTooltip:Hide()
			</OnLeave>
			<OnDragStart>
				self:LockHighlight()
				TrinketMenu.StartTimer("DragMinimapButton")
			</OnDragStart>
			<OnDragStop>
				TrinketMenu.StopTimer("DragMinimapButton")
				self:UnlockHighlight()
			</OnDragStop>
			<OnClick>
				TrinketMenu.MinimapButton_OnClick(button)
			</OnClick>
		</Scripts>
