<Frame name="icralistach" hidden="true" parent="ARmain1" inherits="BackdropTemplate">
	<Scripts>
		<OnLoad>
					self.background = CreateFrame("Frame", nil, self, "BackdropTemplate")
					self.background:SetFrameStrata("BACKGROUND")
					self.background:SetAllPoints()
					self.background:SetBackdrop(
						{
							bgFile = "Interface\\Tooltips\\UI-Tooltip-Background",
							edgeFile = "Interface\\Tooltips\\UI-Tooltip-Border",
							tile = true,
							tileSize = 16,
							edgeSize = 8,
							insets = { left = 3, right = 3, top = 3, bottom = 3, },
						})
					self.background:SetBackdropColor(.03, .03, .03, 0.8)
					self.background:SetBackdropBorderColor(.4, .4, .4)
				</OnLoad>
	</Scripts>


