<UI xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">
	<!-- Listbox List Button -->
	<Button name="NyxGUIListButton" virtual="true">
		<Size><AbsDimension x="80" y="12"/></Size>
	
		<Backdrop bgFile="Interface\Buttons\GreyscaleRamp64" edgeFile="Interface\Buttons\WHITE8X8">
			<Color r="0" g="0" b="0" a="0"/>
			<BorderColor r="0.1" g="0.1" b="0.1" a="0.2"/>
			<EdgeSize val="1"/>
		</Backdrop>

		<HighlightTexture file="Interface\QuestFrame\UI-QuestLogTitleHighlight" alphaMode="ADD">
			<Anchors>
				<Anchor point="TOPLEFT">
					<Offset>
						<AbsDimension x="0" y="1"/>
					</Offset>
				</Anchor>
				<Anchor point="BOTTOMRIGHT">
					<Offset>
						<AbsDimension x="0" y="1"/>
					</Offset>
				</Anchor>
			</Anchors>
		</HighlightTexture>
	</Button>
	
</UI>
